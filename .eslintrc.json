{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": "off",
        "camelcase": "off",
        "no-continue": "off",
        "prefer-destructuring": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "max-classes-per-file": "off"
    }
}
